{
    "name": "repl.it",
    "version": "1.1.0",
    "description": "electron version of repl.it",
    "main": "main.js",
    "scripts": {
        "start": "npm run dev:main:watch",
        "dev:main": "electron src/main/bootstrap.js",
        "dev:main:watch": "nodemon -w src -e js,ts,json,html,css --exec electron src/main/bootstrap.js",
        "lint": "eslint \"**/*.{js,ts}\"",
        "build": "webpack --config webpack.prod.js",
        "build:run": "electron dist/main.bundle.js",
        "package": "node build/building.js"
    },
    "keywords": [
        "electron",
        "repl.it",
        "discord-rpc"
    ],
    "author": "leon332157&repl-it-discord",
    "devDependencies": {
        "@babel/cli": "^7.5.0",
        "@babel/core": "^7.5.0",
        "@babel/plugin-proposal-class-properties": "^7.5.0",
        "@babel/preset-env": "^7.5.3",
        "@babel/preset-typescript": "^7.3.3",
        "@babel/register": "^7.4.4",
        "@types/node": "^12.0.12",
        "@typescript-eslint/eslint-plugin": "^1.11.0",
        "@typescript-eslint/parser": "^1.11.0",
        "acorn": "^6.2.0",
        "babel-loader": "^8.0.6",
        "clean-webpack-plugin": "^3.0.0",
        "discord-rpc": "^3.0.2",
        "electron": "^5.0.6",
        "electron-context-menu": "^0.13.0",
        "electron-packager": "^14.0.1",
        "electron-prompt": "^1.3.1",
        "eslint": "^6.0.1",
        "eslint-config-prettier": "^6.0.0",
        "eslint-plugin-import": "^2.18.0",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-watch": "^5.1.2",
        "eventemitter2": "^5.0.1",
        "nodemon": "^1.19.1",
        "prettier": "^1.18.2",
        "register-scheme": "^0.0.2",
        "restler": "^3.4.0",
        "tslint": "^5.18.0",
        "typescript": "^3.5.2",
        "webpack": "^4.35.3",
        "webpack-cli": "^3.3.5",
        "zip-local": "^0.3.4"
    },
    "dependencies": {
        "@babel/runtime": "^7.5.1",
        "axios": "^0.19.0",
        "fs-extra": "^8.1.0",
        "lodash": "^4.17.11",
        "node-fetch": "^2.6.0"
    },
    "repository": "https://github.com/repl-it-discord/repl.it-electron",
    "license": "Apache-2.0"
}
