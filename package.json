{
    "name": "repl.it",
    "version": "1.1.0",
    "description": "electron version of repl.it",
    "main": "main.js",
    "scripts": {
        "start": "npm run dev:main:watch",
        "dev:main": "electron src/main/bootstrap.js",
        "dev:main:watch": "nodemon -w src -e js,ts,json,html,css --exec electron src/main/bootstrap.js",
        "lint": "eslint \"**/*.{js,ts}\"",
        "build": "webpack --config webpack.prod.js",
        "build:run": "electron dist/main.bundle.js",
        "build:ts": "tsc -b tsconfig.json && npm run build:ts:static && npm run ts:start",
        "build:ts:static": "copyfiles package.json ts-out && copyfiles src/lib/electron-preferences/* ts-out/lib/electron-preferences/ -V -u 3 && copyfiles src/lib/electron-preferences/**/* ts-out/lib/electron-preferences/ -V -u 3"
        ,"ts:start": "electron ./ts-out"
    },
    "keywords": [
        "electron",
        "repl.it",
        "discord-rpc"
    ],
    "author": "leon332157&repl-it-discord",
    "devDependencies": {
        "@babel/cli": "^7.6.2",
        "@babel/core": "^7.6.2",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/preset-env": "^7.6.2",
        "@babel/preset-typescript": "^7.6.0",
        "@babel/register": "^7.6.2",
        "@types/node": "^12.7.8",
        "@typescript-eslint/eslint-plugin": "^2.3.1",
        "@typescript-eslint/parser": "^2.3.1",
        "acorn": "^7.1.0",
        "babel-loader": "^8.0.6",
        "clean-webpack-plugin": "^3.0.0",
        "discord-rpc": "^3.1.0",
        "electron": "^6.0.10",
        "electron-context-menu": "^0.15.0",
        "electron-packager": "^14.0.6",
        "electron-prompt": "^1.3.1",
        "eslint": "^6.4.0",
        "eslint-config-prettier": "^6.3.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-prettier": "^3.1.1",
        "eslint-watch": "^6.0.1",
        "eventemitter2": "^5.0.1",
        "gulp": "^4.0.2",
        "nodemon": "^1.19.2",
        "prettier": "^1.18.2",
        "register-scheme": "^0.0.2",
        "restler": "^3.4.0",
        "ts-node": "^8.4.1",
        "tslint": "^5.20.0",
        "typescript": "^3.6.3",
        "webpack": "^4.41.0",
        "webpack-cli": "^3.3.9",
        "zip-local": "^0.3.4"
    },
    "dependencies": {
        "@babel/runtime": "^7.6.2",
        "axios": "^0.19.0",
        "copyfiles": "^2.1.1",
        "fs-extra": "^8.1.0",
        "lodash": "^4.17.15",
        "node-fetch": "^2.6.0"
    },
    "repository": "https://github.com/repl-it-discord/repl.it-electron",
    "license": "Apache-2.0"
}
